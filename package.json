{
  "name": "oceanai-js",
  "version": "1.0.6",
  "description": "Wrapper library that allows a direct integration between the Ocean marketplace and artificial intelligence tools like tensorflow",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc --skipLibCheck",
    "publish-lib": "npm run build && npm publish",
    "publish-beta": "npm run build && npm publish --tag beta",
    "publish-dryrun": "npm run build && npm publish --dry-run",
    "link": "tsc --skipLibCheck && npm link"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itrm-team/ocean-tensorflowjs.git"
  },
