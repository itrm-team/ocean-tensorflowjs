  "license": "ISC",
  "bugs": {
    "url": "https://github.com/itrm-team/ocean-tensorflowjs/issues"
  },
  "homepage": "https://github.com/itrm-team/ocean-tensorflowjs#readme",
  "dependencies": {
    "@oceanprotocol/lib": "^0.20.2",
    "@tensorflow/tfjs-node": "^3.13.0",
    "@types/node": "^17.0.18",
    "axios": "^0.26.1",
    "fs": "^0.0.1-security",
    "googleapis": "^95.0.0",
    "web3": "^1.7.1"
  },
  "devDependencies": {
    "typescript": "^4.7.4"
  }
}
